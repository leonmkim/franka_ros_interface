<?xml version="1.0" ?>
<launch>
    <arg name="in_sim" default="false" /> 

    <rosparam command="load" file="$(find franka_safety)/config/virtual_walls.yaml"/>


    <node name="virtual_wall" pkg="franka_safety" type="virtual_wall.py" output="screen" required="true" />

    <node pkg="rviz" type="rviz" output="screen" name="rviz" args="-d $(find franka_visualization)/launch/franka_visualization.rviz"/>

</launch>
